@model List<IdentityUser>
@{
    ViewData["Title"] = "AddUserToRole";
}

<h1>Add User To Role</h1>

<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td>
                @Html.ActionLink("Give role", "AddUserToRoleSave", "RolesAdmin", new { role = ViewBag.Role, userId = item.Id })
            </td>
        </tr>
    }
</table>

<a asp-controller="RolesAdmin" asp-action="Index">Back to Roles list</a>